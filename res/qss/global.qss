}
/*QDialog style*/
QDialog{
	background-color: rgb(6,34,104);
	color:blue;
}
QDialog > QLabel {
	color:white;
	background-color: rgb(255, 255, 255,0);
}
QDialog QRadioButton::indicator {
    border-radius:5px;
	padding:1px;
	border:1px solid #191970;
	width: 8px;
	height:8px;
}
QDialog > QComboBox {
	background-color: rgb(222, 232, 255);
	font:20px;
	color:blue;
	border-radius:4px;
	border:1px solid rgb(154, 184, 252);
}
QDialog  QPushButton {
	width:80px;
	height:25px;
