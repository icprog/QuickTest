}
/*QTableView style*/
QTableView { 
    background-color: rgb(255, 255, 255);
	border-top-left-radius:4px;
	border-top-right-radius:4px;
	border-bottom-left-radius:4px;
	border-bottom-right-radius:4px;
}
/*QHeaderView style*/
QHeaderView { 
    /* background-color: rgb(255, 255, 255); */
	background-color:rgb(154, 184, 252);
	color:blue;
	border-top-left-radius:0px;
	border-top-right-radius:0px;
	border-bottom-left-radius:0px;
	border-bottom-right-radius:0px;
}
QHeaderView::section {
     /* background-color:rgb(236, 236, 210); */
     color: blue;
     padding-left: 4px;
     border: 1px solid #6c6c6c;
	 padding:2px;
     /* border: 1px solid white; */
 }
QHeaderView ::section:last{
	min-width:200px;
}
/*QListWidget style*/
QListWidget { 
    background-color: white;
	border-radius:4px;
}
/*QTreeView style*/
QTreeView {
	background-color: white;
	border-top-left-radius:0px;
	border-top-right-radius:0px;
	border-bottom-left-radius:4px;
	border-bottom-right-radius:4px;
}
QTreeView::branch:closed:has-children:!has-siblings,QTreeView::branch:closed:has-children:has-siblings{
	image: url(:/images/unfold.png);
}
QTreeView::branch:open:has-children:!has-siblings,QTreeView::branch:open:has-children:has-siblings{
	image: url(:/images/fold.png);
}
